{
  "name": "@dormakaba/digital-reactnative-visual",
  "main": "index.js",
  "version": "0.0.12",
  "scripts": {
    "start": "npm run storybook",
    "ios": "node node_modules/react-native/local-cli/cli.js run-ios",
    "ios:device": "node node_modules/react-native/local-cli/cli.js run-ios --device",
    "link": "node node_modules/react-native/local-cli/cli.js link",
    "clearCache": "watchman watch-del-all && node node_modules/react-native/local-cli/cli.js start --resetCache",
    "test": "jest",
    "storybook": "storybook start -p 7007 --skip-packager",
    "prestorybook": "rnstl",
    "install:peer": "npm i react-native-vector-icons@4.3.0 react@16.0.0-alpha.12 react-native@0.48.3 react-native-camera@0.10.0 react-native-image-crop-picker@0.16.1",
    "postversion": "git push && git push --tags"
  },
  "dependencies": {
    "color-hash": "^1.0.3",
    "react-native-modal-selector": "0.0.19",
    "react-native-swipeable": "^0.6.0"
  },
  "devDependencies": {
    "@storybook/addon-actions": "3.2.6",
    "@storybook/addon-links": "3.2.6",
    "@storybook/react-native": "3.2.8",
    "babel-jest": "20.0.3",
    "babel-preset-react-native": "4.0.0",
    "babel-preset-react-native-stage-0": "^1.0.1",
    "jest": "20.0.4",
    "prop-types": "15.5.10",
    "react-dom": "16.0.0-alpha.12",
    "react-native-storybook-loader": "1.5.1",
    "react-test-renderer": "16.0.0-alpha.12"
  },
  "peerDependencies": {
    "react-native-vector-icons": "4.3.0",
    "react": "16.0.0-alpha.12",
    "react-native": "0.48.3",
    "react-native-camera": "^0.10.0",
    "react-native-image-crop-picker": "^0.16.1"
  },
  "rnpm": {
    "assets": [
      "./assets/fonts/"
    ]
  },
  "jest": {
    "preset": "react-native"
  },
  "config": {
    "react-native-storybook-loader": {
      "searchDir": [
        "./js"
      ],
      "pattern": "**/*.stories.js",
      "outputFile": "./storybook/storyLoader.js"
    }
  }
}
